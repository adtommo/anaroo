{
  "name": "anaroo",
  "version": "0.1.7",
  "private": true,
  "packageManager": "npm@11.8.0",
  "workspaces": [
    "shared",
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "test": "turbo test",
    "typecheck": "turbo typecheck",
    "lint": "eslint shared/src backend/src frontend/src",
    "lint:fix": "eslint --fix shared/src backend/src frontend/src",
    "storybook": "npm run storybook --workspace=frontend",
    "build-storybook": "npm run build-storybook --workspace=frontend",
    "cy:open": "npm run cy:open --workspace=frontend",
    "cy:run": "npm run cy:run --workspace=frontend",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "prepare": "husky || true"
  },
  "lint-staged": {
    "*.{ts,tsx}": "eslint"
  },
  "devDependencies": {
    "@eslint/js": "^10.0.1",
    "eslint": "^10.0.0",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^17.3.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "turbo": "^2.8.9",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "4.57.1"
  }
}
